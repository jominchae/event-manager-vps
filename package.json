{
  "name": "event-manager-vps",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "description": "Event Manager for Maldives tourism - VPS deployable version",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:clean && npm run build:check && npm run build:client && npm run build:server && npm run build:verify",
    "build:clean": "rm -rf dist",
    "build:check": "tsc --noEmit && echo '✅ TypeScript check passed'",
    "build:client": "vite build && echo '✅ Client built successfully'",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist/server --outfile=dist/server/index.js && echo '✅ Server built successfully'",
    "build:verify": "node -e \"const fs = require('fs'); if (!fs.existsSync('dist/server/index.js')) throw new Error('Server build failed'); if (!fs.existsSync('dist/public/index.html')) throw new Error('Client build failed'); console.log('✅ Build verification passed');\"",
    "start": "NODE_ENV=production node dist/server/index.js",
    "start:prod": "npm run db:migrate && npm run start",
    "check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:migrate": "npm run db:push",
    "preview": "npm run build && npm run start:prod",
    "clean": "rm -rf dist node_modules/.cache",
    "health": "curl -f http://localhost:5000/api/health || exit 1",
    "test": "echo 'Tests not implemented yet' && exit 0"
  },
